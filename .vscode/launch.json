{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) CuraEngine",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/CuraEngine",
            // "args" : [
            // ],
            "args": ["slice",
                "-v",
                "-j",
                "~/Programming/Cura/resources/definitions/prusa_i3_mk2.def.json",
                "-j",
                "~/Programming/Cura/resources/extruders/prusa_i3_mk2_extruder_0.def.json",
                "-o",
                "${workspaceFolder}/output/test.gcode",
                "-s",
                "drum_radius=22.040",
                "-s",
                "drum_coordinate_x=30",
                "-s",
                "drum_coordinate_z=30",
                "-s",
                "machine_center_is_zero=true",
                "-s",
                "adhesion_type=none",
                "-s",
                "layer_height=0.1",
                "-s", 
                "machine_max_feedrate_x=200",
                "-s",
                "machine_max_feedrate_y=200",
                //"-s",
                //"meshfix_keep_open_polygons=true",
                "-s",
                "machine_start_gcode=M109 S215 ; wait for extruder \n G90 ; use absolute positioning \n M82 ;absolute extrusion mode \n G21 ; set units to millimeters \n \n G92 E0.0 ; reset extruder distance position \n G1 X62.8 E9.0 F1000.0 ; intro line \n G92 E0.0 ; reset extruder distance position \n G0 Y3.0 F1000.0 ; go away from print line  \n G92 X0 Y0 E0 ; reset XYE \n G1 F2100 E-0.8 ; slight retract before starting print",
                "-s",
                "machine_end_gcode=G4 ; dwell, make sure the last G-code is done \n M83 ; Set to relative mode for retract \n G1 E-5 F1000 ; retract \n M104 S0 ; set extruder temp to 0 \n G91 ; Set to relative mode for lift \n G1 Z10.0 F1000.0 ; lift up \n G92 E0.0 ; stop extruder just in case",
                "-l",
                "~/Downloads/cubewithsquarehole.stl",
                ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [{"name":"CURA_ENGINE_SEARCH_PATH","value":"/usr/local/src/Cura/resources/definitions:/usr/local/src/Cura/resources/extruders"}],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}