{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) CuraEngine",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/CuraEngine",
            // "args" : [
            // ],
            "args": ["slice",
                "-v",
                "-j",
                "~/Programming/Cura/resources/definitions/prusa_i3_mk2.def.json",
                "-j",
                "~/Programming/Cura/resources/extruders/prusa_i3_mk2_extruder_0.def.json",
                "-o",
                "${workspaceFolder}/output/test.gcode",
                "-s",
                "drum_radius=22.040",
                "-s",
                "drum_coordinate_x=30",
                "-s",
                "drum_coordinate_z=30",
                "-s",
                "machine_center_is_zero=true",
                "-s",
                "adhesion_type=none",
                "-s",
                "layer_height=0.2",
                "-s", 
                "machine_max_feedrate_x=200",
                "-s",
                "machine_max_feedrate_y=200",
                "-s",
                "zig_zaggify_infill=true",
                "-s",
                "infill_pattern=lines",
                //"-s",
                //"meshfix_keep_open_polygons=true",
                "-l",
                "~/Downloads/60mmcube.stl",
                ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [{"name":"CURA_ENGINE_SEARCH_PATH","value":"/usr/local/src/Cura/resources/definitions:/usr/local/src/Cura/resources/extruders"}],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}