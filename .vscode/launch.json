{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) External Backend",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/CuraEngine",
            "args": ["connect", 
                "127.0.0.1:49674",
                "-j",
                "~/Programming/Cura/resources/definitions/cylindrical_zaribo.def.json",
                "-j",
                "~/Programming/Cura/resources/extruders/prusa_i3_mk2_extruder_0.def.json"
                ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [{"name":"CURA_ENGINE_SEARCH_PATH","value":"/home/richard/Programming/Cura/resources/definitions:/home/richard/Programming/Cura/resources/extruders"}],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) CuraEngine",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/CuraEngine",
            // "args" : [
            // ],
            "args": ["slice",
                "-v",
                "-j",
                "~/Programming/Cura/resources/definitions/cylindrical_zaribo.def.json",
                "-j",
                "~/Programming/Cura/resources/extruders/prusa_i3_mk2_extruder_0.def.json",
                "-o",
                "~/Downloads/test.gcode",
                "-s",
                "drum_radius=12.70",
                "-s",
                "drum_coordinate_x=0",
                "-s",
                "drum_coordinate_z=0",
                "-s",
                "seam_front=16907",
                "-s",
                "seam_back=46123",
                "-s",
                "cyl_skin_density_enabled=true",
                "-s",
                "fiber_cut_enabled=false",
                "-s",
                "fiber_cut_length=80.00",
                "-s", 
                "fiber_cut_travel_threshold=0.50",
                "-s",
                "machine_center_is_zero=true",
                "-s",
                "adhesion_type=none",
                "-s",
                "layer_height=0.5",
                "-s",
                "skin_angles=[30,-30]",
                "-s",
                "infill_angles=[30,-30]",
                //"-s",
                //"zig_zaggify_infill=true",
                //notably zig-zaggify w/ lines produces different results than just zigzag pattern
                "-s",
                "infill_pattern=zigzag",
                "-s",
                "infill_line_distance=2.3",
                "-s",
                "top_bottom_pattern=zigzag",
                "-s",
                "top_bottom_pattern_0=zigzag",
                "-s",
                "top_layers=2",
                "-s",
                "bottom_layers=2",
                "-s",
                "magic_spiralize=false",
                "-s",
                "wall_line_count=2",
                "-s",
                "wall_line_width_0=0.35",
                "-s",
                "wall_line_width_x=0.35",
                "-s",
                "speed_travel=20",
                "-s",
                "speed_wall_x=20",
                "-s",
                "speed_wall_0=20",
                "-s",
                "speed_travel_layer_0=20",
                "-s",
                "speed_print_layer_0=20",
                "-s",
                "speed_support_infill=20",
                "-s",
                "speed_topbottom=20",
                "-s",
                "meshfix_keep_open_polygons=false",
                "-s",
                "filter_out_tiny_gaps=true",
                "-s",
                "fill_perimeter_gaps=nowhere",
                "-l",
                "~/Downloads/25.4diam/cube.stl",
                ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [{"name":"CURA_ENGINE_SEARCH_PATH","value":"/usr/local/src/Cura/resources/definitions:/usr/local/src/Cura/resources/extruders"}],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}